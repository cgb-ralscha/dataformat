namespace ch.rasc.dataformat.fb;

table Address {
  id:uint; 
  lastName:string;
  firstName:string;
  street:string;
  zip:string;
  city:string;
  country:string;
  lat:float;
  lng:float;
  email:string;  
  dob:int;
}

table Addresses {
  address:[Address];
}

root_type Addresses;